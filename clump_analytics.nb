  FormBox[
   RowBox[{
    RowBox[{
     FractionBox["\[PartialD]", 
      RowBox[{"\[PartialD]", " ", 
       SubscriptBox["d", "s"]}]], 
     FractionBox[
      RowBox[{
       SuperscriptBox["d", "2"], 
       SubscriptBox["\[Phi]", 
        RowBox[{"e", "-"}]]}], 
      RowBox[{"dE", " ", "dr"}]]}], "=", 
