Cell[BoxData[
 RowBox[{
  RowBox[{"Lclump", "[", "\[Rho]_", "]"}], ":=", 
  RowBox[{"Simplify", "[", 
   RowBox[{"Integrate", "[", 
    RowBox[{
     RowBox[{"4", "\[Pi]", " ", 
      SuperscriptBox["r", "2"], 
      SuperscriptBox[
       RowBox[{"\[Rho]", "[", "r", "]"}], "2"]}], ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "0", ",", "\[Infinity]"}], "}"}]}], "]"}], 
   "]"}]}]], "Input",
