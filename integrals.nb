              RowBox[{"\[Psi]ND", "[", "xp", "]"}]}], "/.", "params"}], "/.", 
            "GSub"}], "]"}], "\[Equal]", "0"}], ",", 
         RowBox[{"{", 
          RowBox[{"xp", ",", 
           RowBox[{
            FractionBox["r", "rRef"], "/.", "params"}]}], "}"}]}], "]"}]}]}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Perform", " ", "the", " ", "integration"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"integrand", "[", "xp_", "]"}], "=", 
