Cell[BoxData[
 RowBox[{
  RowBox[{"get\[Psi]\[Rho]Derivs", "[", 
   RowBox[{"\[Rho]_", ",", 
    RowBox[{"extraAssumps_:", 
     RowBox[{"{", "}"}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "d\[Rho]\[LetterSpace]dr", ",", "d2\[Rho]\[LetterSpace]dr2", ",", "m", 
      ",", "\[Psi]", ",", "d\[Psi]\[LetterSpace]dr", ",", 
      "d2\[Psi]\[LetterSpace]dr2"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"m", "[", "r_", "]"}], "=", 
      RowBox[{"Simplify", "[", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"4", "\[Pi]", " ", 
          SuperscriptBox["rp", "2"], 
          RowBox[{"\[Rho]", "[", "rp", "]"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"rp", ",", "0", ",", "r"}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"Assumptions", "\[Rule]", 
          RowBox[{"Join", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"r", ">", "0"}], "}"}], ",", "extraAssumps"}], "]"}]}]}],
         "]"}], "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"\[Psi]", "[", "r_", "]"}], "=", 
      RowBox[{"Simplify", "[", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         FractionBox[
          RowBox[{"G", " ", 
